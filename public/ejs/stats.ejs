<html>
  <head>
    <title>SB Stats</title>
    <meta property="og:site_name" content="SBStats">

    <meta property="og:type" content="profile">
    <meta property="og:title" content="<%=player%><% if (typeof profile != 'undefined') {%> - <%=profile%><%}%>">
    <meta property="og:description" content="<% if (typeof description != 'undefined') {%> <%=description%> <%} else {%> <%='This profile is not yet loaded. Try again soon.'%> <%}%>">
    <% if (typeof image != 'undefined') {%>
      <meta property="og:image" content="<%=image%>">
    <%}%>
    
    <link rel='search' type='application/opensearchdescription+xml' title='SB Stats' href='/opensearch.xml' />
    <link href="/css/stats.css" rel="stylesheet" type="text/css" />
    <link href="/css/playerModel.css" rel="stylesheet" type="text/css" />
    <link href="/css/inventory.css" rel="stylesheet" type="text/css" />
    <link href="https://fonts.googleapis.com/css?family=Quicksand&display=swap" rel="stylesheet">
    <link id="favicon" rel="shortcut icon" type="image/png" href="/img/favicon.png" />
    <img src="/img/items.png" class="itemImg">
    <img src="/img/items.webp" class="itemImg">
  </head>
  <body>
    <!-- nav -->
    <div id="header">
      <link href="/css/header.css" rel="stylesheet" type="text/css" />
      <a id="header-link" href="/">SB Stats</a>
      <div id="header-search">
        <div id="search-type-selector">
          <div id="search-type-selector-selected">User ></div>
          <div class="selector-opt">User</div>
          <div class="selector-opt">Guild</div>
        </div>
        <input id="header-search-text" placeholder="Name" type="text">
        <input id="header-search-button" type="button">
        <span id="header-search-icon">&#9906;</span>
      </div>
      <span id="header-version">v0.1</span>
      <script src="/js/header.js"></script>
      <div id="item-hover">
        <div id="item-hover-header"></div>
        <div id="item-hover-lore"></div>
      </div>
    </div>
    <!-- end nav -->
